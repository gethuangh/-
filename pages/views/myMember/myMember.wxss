.myMember-top {
    margin-top: 20rpx;
    height: 126rpx;
    background: linear-gradient(90deg, #FFB41D 0%, #FFC523 100%);
    box-shadow: 0rpx 20rpx 30rpx 0rpx rgba(254, 177, 45, 0.2);
    border-radius: 24rpx;
    justify-content: space-between;
    align-items: center;
    font-size: 36rpx;
    font-weight: 400;
    color: #FFFFFF;
    padding: 0 31rpx;
    box-sizing: border-box;
}

.myMember-yq {
    margin-top: 40rpx;
    text-align: center;

    height: 200rpx;
    background: #FFFFFF;
    box-shadow: 0px 0px 24rpx 0px rgba(6, 0, 1, 0.09);
    border-radius: 24rpx;

    font-size: 36rpx;
    font-weight: 600;
    color: #FFB51D;
}

.myMember-yq>image {
    width: 55rpx;
    height: 55rpx;
    margin-top: 43rpx;
    margin-bottom: 13rpx;
}

.myMember-cont {
    margin-top: 35rpx;
}

.myMember-item {
    justify-content: space-between;
    height: 200rpx;
    background: #FFFFFF;
    box-shadow: 0px 0px 24rpx 0px rgba(6, 0, 1, 0.09);
    border-radius: 24rpx;
    padding: 44rpx;
    box-sizing: border-box;
    margin-bottom: 30rpx;
}

.myMember-item-left>image {
    width: 114rpx;
    height: 114rpx;
    border-radius: 50%;
}

.myMember-item-cont {
    margin-left: 30rpx;
}

.myMember-item-name {
    font-size: 32srpx;
    font-weight: 400;
    color: #333333;
}

.myMember-item-text {
    margin-top: 20rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: #999999;
}

.myMember-item-time {
    font-size: 24rpx;
    font-weight: 400;
    color: #999999;
}