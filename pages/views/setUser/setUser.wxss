.setUser-head {}

.setUser {
    position: relative;
}

.setUser-box {
    font-size: 34rpx;
    font-weight: 400;
    width: 100%;
    color: #282828;
    padding: 30rpx 35rpx;
    box-sizing: border-box;
    box-shadow: 0px 0px 24rpx 0px rgba(6, 0, 1, 0.09);
    border-radius: 24rpx;
    margin-top: 30rpx;
}

.setUser-boxItem {
    justify-content: space-between;
    align-items: center;
    height: 100rpx;
    margin-top: 10rpx;
}

.setUser-right {
    align-items: center;
}

.setUser-heads {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin-right: 25rpx;
}

.setUser-you {
    width: 14rpx;
    height: 26rpx;
}

.picker {
    width: 100%;
    height: 100%;
}

.setUser-text {
    font-size: 30rpx;
    font-weight: 400;
    color: #999999;
    margin-right: 20rpx;
    width: 216rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: right;
}

/* 推出登录 */
.setUser-btn {
    width: 570rpx;
    height: 90rpx;
    line-height: 90rpx;
    background: #FFC50A;
    box-shadow: 0rpx 20rpx 30rpx 0rpx rgba(254, 177, 45, 0.2);
    border-radius: 40rpx;
    position: absolute;
    left: 89rpx;
    bottom: 50rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 600;
    color: #5B2F04;
}